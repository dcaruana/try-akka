<configuration>

  <appender name="MAIN" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} %-5level %-10contextName %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>
  
  <logger name="caruana" level="INFO"/>

  <root level="DEBUG">
    <appender-ref ref="MAIN"/>
  </root>
  
</configuration>
